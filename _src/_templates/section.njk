<section class="sub sub-{{  section.data.pageClass }}" role="region" aria-labelledby="latest-{{  section.data.pageClass }}">
	<div class="section-head">
		<h2 class="section-hed txt-shaded" id="latest-{{ section.data.pageClass }}">{{ section.data.title | safe }}</h2>
	</div>
	{% if section.data.img %}
	<img src="/_assets/img/{{ section.data.img }}">
	{% endif %}
	<div class="copy">
		{{ section.data.content | safe }}
	</div>
</section>